ngx_addon_name=ngx_http_set_hash_module
HTTP_MODULES="$HTTP_MODULES ngx_http_set_hash_module"
NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_set_hash_module.c"

have=NDK_SET_VAR        . auto/have
have=NDK_HASH           . auto/have
have=NDK_HASH_ALL       . auto/have
have=NGX_HTTP_SET_HASH  . auto/have

USE_OPENSSL=YES
USE_MD5=YES
USE_SHA1=YES